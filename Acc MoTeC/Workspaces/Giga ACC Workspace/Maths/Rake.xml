<?xml version="1.0"?>
<Maths Locale="English_United Kingdom.1252" DefaultLocale="C" Id="Rake">
 <MathConstants>
  <MathConstant Name="PI" Value="3.14159265" Unit=""/>
 </MathConstants>
 <MathItems>
  <MathExpression Id="Section Dist" DisplayUnit="m" DisplayDPS="2" DisplayColorIndex="4" Interpolate="0" Script="'Corr Dist' [m] - stat_start('Corr Dist' [m], 1, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="m"/>
  <MathExpression Id="Sect Dist Max" DisplayUnit="m" DisplayDPS="2" DisplayColorIndex="1" Interpolate="0" Script="stat_max('Section Dist' [m], 1, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="m"/>
  <MathExpression Id="Rake 1st Half" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="choose('Section Dist' [m] &lt; 'Sect Dist Max' [m] * 0.5, 'Rake' [mm], invalid())" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Rake 2nd Half" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="choose('Section Dist' [m] &gt; 'Sect Dist Max' [m] * 0.5, 'Rake' [mm], invalid())" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Avg Rake 1st Half" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="stat_mean('Rake 1st Half' [mm], 1, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Avg Rake 2nd Half" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="13" Interpolate="0" Script="stat_mean('Rake 2nd Half' [mm], 1, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Avg Section Rake Difference " DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="7" Interpolate="0" Script="'Avg Rake 2nd Half' [mm] - 'Avg Rake 1st Half' [mm]" SampleRate="0" Unit="mm"/>
 </MathItems>
</Maths>
