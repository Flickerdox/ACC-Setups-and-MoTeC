<?xml version="1.0"?>
<Maths Locale="English_United Kingdom.1252" DefaultLocale="C" Id="Oversteer And Understeer">
 <MathConstants>
  <MathConstant Name="Front Track" Value="1.679" Unit="m"/>
  <MathConstant Name="PI" Value="3.14159265" Unit=""/>
  <MathConstant Name="Radian To Deg" Value="57.2957795131" Unit="deg"/>
  <MathConstant Name="Rear Track" Value="1.651" Unit="m"/>
  <MathConstant Name="Steering Ratio In Settings" Value="13" Unit="" Comment="Lambo: 11   Merc AMG 2020: 14 Ferrari EVO: 14  Mclaren 720s: 13"/>
  <MathConstant Name="Vehicle Wheelbase" Value="2.645" Unit="m"/>
 </MathConstants>
 <MathItems>
  <MathExpression Id="Steered Angle Manic" DisplayUnit="deg" DisplayDPS="2" DisplayColorIndex="0" Interpolate="1" Script="(1/ 'Steering Ratio In Settings')

* 'STEERANGLE' [deg]" SampleRate="0" Unit="deg"/>
  <MathExpression Id="Ackermann Angle" DisplayUnit="deg" DisplayDPS="2" DisplayColorIndex="3" Interpolate="1" Script="choose('corr speed' [km/h] &gt; 30,
'G Lateral' [m/s/s]
/
sqr('Speed' [m/s])
*
'Vehicle Wheelbase' [m]
*
'Radian To Deg' [deg]
, invalid())" SampleRate="20" Unit="deg"/>
  <MathExpression Id="Understeer Angle" DisplayUnit="deg" DisplayDPS="4" DisplayColorIndex="7" Interpolate="1" Script="
choose('corr speed' [km/h] &lt; 30, invalid(), abs('Steered Angle Manic' [deg]) - abs('Ackermann Angle' [deg]))
" SampleRate="0" Unit="deg"/>
  <MathExpression Id="G Lateral" DisplayUnit="G" DisplayDPS="2" DisplayColorIndex="3" Interpolate="1" Script="'G_LAT' [m/s2]" SampleRate="0" Unit="G"/>
  <MathExpression Id="Attitude Velocity" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="12" Interpolate="1" Script="
choose('Corr Speed' [km/h] &gt; 30, 'ROTY ABS' [deg/s] - abs('Angular Velocity' [deg/s]), invalid())
" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Int" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="0" Interpolate="1" Script="integrate('Attitude Velocity' [deg/s])" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Int Oversteer Lap" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="0" Interpolate="1" Script="integrate('Attitude Velocity' [deg/s], 'Attitude Velocity' [deg/s] &gt; 0, range_change(&quot;Outings:Laps&quot;))" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Oversteer Int " DisplayUnit="deg/s" DisplayDPS="2" DisplayColorIndex="0" Interpolate="1" Script="integrate('Attitude Velocity' [deg/s], 'Attitude Velocity' [deg/s] &gt; 0)" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Understeer Int" DisplayUnit="deg/s" DisplayDPS="2" DisplayColorIndex="7" Interpolate="1" Script="integrate('Attitude Velocity' [deg/s], 'Attitude Velocity' [deg/s] &lt; 0)" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Int Understeer Lap" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="7" Interpolate="1" Script="integrate('Attitude Velocity' [deg/s], 'Attitude Velocity' [deg/s] &lt; 0, range_change(&quot;Outings:Laps&quot;))" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Int Lap" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="0" Interpolate="1" Script="integrate('Attitude Velocity' [deg/s], 1, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Int Lap Smooth" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="6" Interpolate="1" Script="smooth('Att Vel Int Lap' [deg/s], 0.5)" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Att Vel Int Smooth" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="6" Interpolate="1" Script="smooth('Att Vel Int' [deg/s], 0.5)" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Attitude Velocity Smoothness" DisplayUnit="deg/s" DisplayDPS="3" DisplayColorIndex="4" Interpolate="1" Script="abs('Att Vel Int' [deg/s] - 'Att Vel Int Smooth' [deg/s]) * 100" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Angular Velocity" DisplayUnit="deg/s" DisplayDPS="2" DisplayColorIndex="3" Interpolate="1" Script="(
'G Lateral' [m/s/s]
/
'Corr Speed' [m/s])
*
57.296" SampleRate="20" Unit="deg/s"/>
  <MathExpression Id="ROTY ABS" DisplayUnit="deg/s" DisplayDPS="4" DisplayColorIndex="14" Interpolate="1" Script="sqrt(sqr('ROTY' [deg/s]))" SampleRate="20" Unit="deg/s"/>
 </MathItems>
</Maths>
