<?xml version="1.0"?>
<Maths Locale="English_United Kingdom.1252" DefaultLocale="C" Id="Variance">
 <MathConstants>
  <MathConstant Name="PI" Value="3.14159265" Unit=""/>
 </MathConstants>
 <MathItems>
  <MathExpression Id="Instantaneous Variance" DisplayUnit="s" DisplayDPS="2" DisplayColorIndex="8" Interpolate="0" Script="derivative(variance_dist('Calc Outing Time' [s]), 101)" SampleRate="0" Unit="s"/>
  <MathExpression Id="Cumulative Variance" DisplayUnit="s" DisplayDPS="2" DisplayColorIndex="8" Interpolate="1" Script="variance_dist('Calc Lap Time' [s])" SampleRate="0" Unit="s"/>
  <MathExpression Id="Delta Section Variance" DisplayUnit="s" DisplayDPS="2" DisplayColorIndex="8" Interpolate="0" Script="integrate('Instantaneous Variance' [s], 1, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="s"/>
  <MathExpression Id="Variance Gain" DisplayUnit="s" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="integrate('Instantaneous Variance' [s], 'Instantaneous Variance' [s] &gt; 0, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="s"/>
  <MathExpression Id="Variance Loss" DisplayUnit="s" DisplayDPS="2" DisplayColorIndex="0" Interpolate="1" Script="integrate('Instantaneous Variance' [s], 'Instantaneous Variance' [s] &lt; 0, range_change(&quot;Outings:Laps:Track Sections:Default&quot;))" SampleRate="0" Unit="s"/>
 </MathItems>
</Maths>
